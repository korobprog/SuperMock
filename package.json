{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20",
    "pnpm": ">=8 <9"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "pnpm run db:up && node start-dev-improved.cjs",
    "dev:fast": "node start-dev-improved.cjs",
    "dev:prod": "node start-prod-local.cjs",
    "client": "pnpm exec vite --config frontend/vite.config.ts",
    "client:simple": "node start-client-simple.js",
    "vite": "node start-vite.js",
    "server": "cross-env PORT=3000 DATABASE_URL=\"postgresql://supermock:postgres@localhost:5432/supermock?schema=public\" DATABASE_URL_SECONDARY=\"postgresql://supermock2:postgres@localhost:5433/supermock_secondary?schema=public\" NODE_ENV=development ENABLE_DEV_ENDPOINTS=1 node backend/server/index.js",
    "server:prod": "cross-env NODE_ENV=production PORT=3001 node backend/server/index.js",
    "build": "vite build --config frontend/vite.config.ts",
    "preview": "vite preview --config frontend/vite.config.ts",
    "lint": "eslint .",
    "db:up": "docker compose -f docker-compose.dev.yml --env-file ./.env up -d postgres postgres_secondary redis",
    "db:down": "docker compose -f docker-compose.dev.yml down -v",
    "db:studio": "npx prisma studio --schema backend/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema backend/prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema backend/prisma/schema.prisma",
    "telegram:setup": "node scripts/setup-telegram-webhook.js",
    "telegram:test": "node scripts/test-telegram-bot.js",
    "telegram:status": "curl -s https://api.telegram.org/bot$TELEGRAM_BOT_TOKEN/getMe | jq .",
    "deploy-interactive": "sh scripts/deploy/deploy-interactive.sh",
    "deploy-frontend": "sh scripts/deploy/deploy-frontend.sh",
    "deploy-backend": "sh scripts/deploy/deploy-backend.sh"
  },
