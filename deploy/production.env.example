# Production Environment Variables for Dokploy
# Copy these variables to Dokploy Environment settings

# Database Configuration
DATABASE_URL=postgresql://Super Mock:YOUR_SECURE_DB_PASSWORD@postgres:5432/Super Mock
POSTGRES_DB=Super Mock
POSTGRES_USER=Super Mock
POSTGRES_PASSWORD=YOUR_SECURE_DB_PASSWORD

# Application Configuration  
NODE_ENV=production
PORT=3000

# Frontend and Backend URLs
FRONTEND_URL=https://supermock.ru
BACKEND_URL=https://api.supermock.ru/api

# API URLs for frontend (build time)
VITE_API_URL=https://api.supermock.ru
VITE_API_URL_HTTPS=https://api.supermock.ru

# Jitsi base URL (if using self-hosted Jitsi)
VITE_JITSI_URL=https://jitsi.supermock.ru

# WebRTC ICE
VITE_STUN_URLS=stun:stun.l.google.com:19302
# Optional TURN
# VITE_TURN_URL=turn:turn.supermock.ru:3478
# VITE_TURN_USERNAME=turn_user
# VITE_TURN_PASSWORD=turn_pass

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN
VITE_TELEGRAM_BOT_NAME=YOUR_BOT_USERNAME

# Domain Configuration
DOMAIN_NAME=api.supermock.ru
